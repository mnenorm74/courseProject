<Page x:Class="registration.Start"
      x:Name="startPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:registration"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Start"
      Background="White">

    <Viewbox Stretch="Uniform">
        <Grid x:Name="startGrid" Focusable="True" Loaded="startGrid_Loaded" KeyDown="start_KeyDown">
            <Image x:Name="startImage" Source="Resources/startBackground.png" />
            <Image x:Name="toGreeting" Source="Resources/right.png" Width="300" Margin="1568,968,0,10" MouseDown="to_MouseDown"/>
        </Grid>
    </Viewbox>

</Page>
